#: gdk-pixbuf/gdk-pixbuf-animation.c:190 gdk-pixbuf/gdk-pixbuf-io.c:1216
#: gdk-pixbuf/gdk-pixbuf-io.c:1489
#, c-format
msgid "Failed to open file “%s”: %s"
msgstr "\"%s\" файлын ашу сәтсіз: %s"

#: gdk-pixbuf/gdk-pixbuf-animation.c:203 gdk-pixbuf/gdk-pixbuf-io.c:1077
#, c-format
msgid "Image file “%s” contains no data"
msgstr "\"%s\" сурет файлында деректер жоқ"

#: gdk-pixbuf/gdk-pixbuf-animation.c:241
#, c-format
msgid ""
"Failed to load animation “%s”: reason not known, probably a corrupt "
"animation file"
msgstr ""
"\"%s\" анимациясын жүктеу сәтсіз: себебі белгісіз, мүмкін анимация файлы "
"зақымдалған"

#: gdk-pixbuf/gdk-pixbuf-animation.c:309 gdk-pixbuf/gdk-pixbuf-io.c:1252
#: gdk-pixbuf/gdk-pixbuf-io.c:1541
#, c-format
msgid ""
"Failed to load image “%s”: reason not known, probably a corrupt image file"
msgstr ""
"\"%s\" суретін жүктеу сәтсіз: себебі белгісіз, мүмкін сурет файлы "
"зақымдалған"

#: gdk-pixbuf/gdk-pixbuf.c:248
msgid "Number of Channels"
msgstr "Арналар саны"

#: gdk-pixbuf/gdk-pixbuf.c:249
msgid "The number of samples per pixel"
msgstr "Пиксельдегі таңдамалар саны"

#: gdk-pixbuf/gdk-pixbuf.c:264
msgid "Colorspace"
msgstr "Түстер кеңістігі"

#: gdk-pixbuf/gdk-pixbuf.c:265
msgid "The colorspace in which the samples are interpreted"
msgstr "Таңдамалар интерпретацияланатын түс кеңістігі"

#: gdk-pixbuf/gdk-pixbuf.c:277
msgid "Has Alpha"
msgstr "Мөлдірлілігі бар"

#: gdk-pixbuf/gdk-pixbuf.c:278
msgid "Whether the pixbuf has an alpha channel"
msgstr "Пиксельдік буффер ішінде альфа-арнасының бар-жоғы"

#: gdk-pixbuf/gdk-pixbuf.c:291
msgid "Bits per Sample"
msgstr "Таңдамаға келетін биттер саны"

#: gdk-pixbuf/gdk-pixbuf.c:292
msgid "The number of bits per sample"
msgstr "Әр таңдамадағы биттер саны"

#: gdk-pixbuf/gdk-pixbuf.c:305
msgid "Width"
msgstr "Ені"

#: gdk-pixbuf/gdk-pixbuf.c:306
msgid "The number of columns of the pixbuf"
msgstr "Пиксельдік буффер бағандарының саны"

#: gdk-pixbuf/gdk-pixbuf.c:319
msgid "Height"
msgstr "Биіктігі"

#: gdk-pixbuf/gdk-pixbuf.c:320
msgid "The number of rows of the pixbuf"
msgstr "Пиксельдік буффер жолдарының саны"

#: gdk-pixbuf/gdk-pixbuf.c:337
msgid "Rowstride"
msgstr "Жол қадамы"

#: gdk-pixbuf/gdk-pixbuf.c:338
msgid ""
"The number of bytes between the start of a row and the start of the next row"
msgstr "Жолдың басы мен келесі жолдың басы арасындағы байттар саны"

#: gdk-pixbuf/gdk-pixbuf.c:351
msgid "Pixels"
msgstr "Пиксель"

#: gdk-pixbuf/gdk-pixbuf.c:352
msgid "A pointer to the pixel data of the pixbuf"
msgstr "Пиксельдік буффердің пиксель деректеріне сілтегіш"

#: gdk-pixbuf/gdk-pixbuf.c:366
msgid "Pixel Bytes"
msgstr "Пиксель байттары"

#: gdk-pixbuf/gdk-pixbuf.c:367
msgid "Readonly pixel data"
msgstr "Тек оқу үшін пиксель деректері"

#: gdk-pixbuf/gdk-pixbuf-io.c:893
#, c-format
msgid "Unable to load image-loading module: %s: %s"
msgstr "Суретті жүктеу модулін жүктеу мүмкін емес: %s: %s"

#: gdk-pixbuf/gdk-pixbuf-io.c:911
#, c-format
msgid ""
"Image-loading module %s does not export the proper interface; perhaps it’s "
"from a different gdk-pixbuf version?"
msgstr ""
"%s суретті жүктеу модулі тиісті интерфейсті экспорттамайды; бәлкім, ол "
"басқа gdk-pixbuf нұсқасынан шығар?"

#: gdk-pixbuf/gdk-pixbuf-io.c:921 gdk-pixbuf/gdk-pixbuf-io.c:964
#, c-format
msgid "Image type “%s” is not supported"
msgstr "\"%s\" суретінің түріне қолдау жоқ"

#: gdk-pixbuf/gdk-pixbuf-io.c:1049
#, c-format
msgid "Couldn’t recognize the image file format for file “%s”"
msgstr "“%s” файлы үшін сурет файлының пішімін тану мүмкін болмады"

#: gdk-pixbuf/gdk-pixbuf-io.c:1057
msgid "Unrecognized image file format"
msgstr "Танылмаған сурет файлының пішімі"

#: gdk-pixbuf/gdk-pixbuf-io.c:1263
#, c-format
msgid "Failed to load image “%s”: %s"
msgstr "\"%s\" суретін жүктеу сәтсіз аяқталды: %s"

#: gdk-pixbuf/gdk-pixbuf-io.c:2338 gdk-pixbuf/io-gdip-utils.c:840
#, c-format
msgid "Error writing to image file: %s"
msgstr "Сурет файлына жазу қатесі: %s"

#: gdk-pixbuf/gdk-pixbuf-io.c:2380 gdk-pixbuf/gdk-pixbuf-io.c:2501
#, c-format
msgid "This build of gdk-pixbuf does not support saving the image format: %s"
msgstr ""
"gdk-pixbuf бағдарламасының бұл жинағы келесі сурет пішімін сақтауды "
"қолдамайды: %s"

#: gdk-pixbuf/gdk-pixbuf-io.c:2411
msgid "Insufficient memory to save image to callback"
msgstr "Суретті кері шақыруға сақтау үшін жады жеткіліксіз"

#: gdk-pixbuf/gdk-pixbuf-io.c:2424
msgid "Failed to open temporary file"
msgstr "Уақытша файлын ашу сәтсіз аяқталды"

#: gdk-pixbuf/gdk-pixbuf-io.c:2447
msgid "Failed to read from temporary file"
msgstr "Уақытша файлдан оқу сәтсіз аяқталды"

#: gdk-pixbuf/gdk-pixbuf-io.c:2669
#, c-format
msgid "Failed to open “%s” for writing: %s"
msgstr "Жазу үшін \"%s\" файлын ашу сәтсіз: %s"

#: gdk-pixbuf/gdk-pixbuf-io.c:2695
#, c-format
msgid ""
"Failed to close “%s” while writing image, all data may not have been saved: "
"%s"
msgstr ""
"Суретті жазу кезінде “%s” жабу сәтсіз аяқталды, барлық деректер сақталмаған "
"болуы мүмкін: %s"

#: gdk-pixbuf/gdk-pixbuf-io.c:2929 gdk-pixbuf/gdk-pixbuf-io.c:2984
msgid "Insufficient memory to save image into a buffer"
msgstr "Суретті буферге сақтау үшін жады жеткіліксіз"

#: gdk-pixbuf/gdk-pixbuf-io.c:3030
msgid "Error writing to image stream"
msgstr "Сурет ағынына жазу қатесі"

#: gdk-pixbuf/gdk-pixbuf-loader.c:426
#, c-format
msgid ""
"Internal error: Image loader module “%s” failed to complete an operation, "
"but didn’t give a reason for the failure"
msgstr ""
"Ішкі қате: “%s” сурет жүктеу модулі операцияны аяқтай алмады, бірақ "
"сәтсіздіктің себебін көрсетпеді"

#: gdk-pixbuf/gdk-pixbuf-loader.c:468
#, c-format
msgid "Incremental loading of image type “%s” is not supported"
msgstr "“%s” сурет түрін инкременталды жүктеуге қолдау көрсетілмейді"

#: gdk-pixbuf/gdk-pixbuf-simple-anim.c:166
msgid "Loop"
msgstr "Цикл"

#: gdk-pixbuf/gdk-pixbuf-simple-anim.c:167
msgid "Whether the animation should loop when it reaches the end"
msgstr "Анимация соңына жеткенде циклге өтуі керек пе"

#: gdk-pixbuf/gdk-pixdata.c:178
msgid "Image header corrupt"
msgstr "Сурет тақырыптамасы зақымдалған"

#: gdk-pixbuf/gdk-pixdata.c:183
msgid "Image format unknown"
msgstr "Сурет пішімі белгісіз"

#: gdk-pixbuf/gdk-pixdata.c:188 gdk-pixbuf/gdk-pixdata.c:491
#: gdk-pixbuf/gdk-pixdata.c:501 gdk-pixbuf/gdk-pixdata.c:597
msgid "Image pixel data corrupt"
msgstr "Суреттің пиксель деректері зақымдалған"

#: gdk-pixbuf/gdk-pixdata.c:513
#, c-format
msgid "failed to allocate image buffer of %u byte"
msgid_plural "failed to allocate image buffer of %u bytes"
msgstr[0] "%u байттық сурет буферін бөлу сәтсіз аяқталды"

#: gdk-pixbuf/io-ani.c:239
msgid "Unexpected icon chunk in animation"
msgstr "Анимациядағы күтпеген таңбаша бөлігі"

#: gdk-pixbuf/io-ani.c:303 gdk-pixbuf/io-ani.c:404 gdk-pixbuf/io-ani.c:430
#: gdk-pixbuf/io-ani.c:449
msgid "Malformed chunk in animation"
msgstr "Анимациядағы қате пішімделген бөлік"

#: gdk-pixbuf/io-ani.c:311 gdk-pixbuf/io-ani.c:354 gdk-pixbuf/io-ani.c:412
#: gdk-pixbuf/io-ani.c:438 gdk-pixbuf/io-ani.c:461 gdk-pixbuf/io-ani.c:488
#: gdk-pixbuf/io-ani.c:575
msgid "Invalid header in animation"
msgstr "Анимациядағы жарамсыз тақырыптама"

#: gdk-pixbuf/io-ani.c:364 gdk-pixbuf/io-ani.c:386 gdk-pixbuf/io-ani.c:470
#: gdk-pixbuf/io-ani.c:497 gdk-pixbuf/io-ani.c:548 gdk-pixbuf/io-ani.c:624
msgid "Not enough memory to load animation"
msgstr "Анимацияны жүктеу үшін жады жеткіліксіз"

#: gdk-pixbuf/io-ani.c:646
msgid "ANI image was truncated or incomplete."
msgstr "ANI суреті қиылған немесе толық емес."

#: gdk-pixbuf/io-ani.c:687
msgctxt "image format"
msgid "Windows animated cursor"
msgstr "Windows анимацияланған курсоры"

#: gdk-pixbuf/io-bmp.c:234 gdk-pixbuf/io-bmp.c:275 gdk-pixbuf/io-bmp.c:385
#: gdk-pixbuf/io-bmp.c:412 gdk-pixbuf/io-bmp.c:437 gdk-pixbuf/io-bmp.c:472
#: gdk-pixbuf/io-bmp.c:494 gdk-pixbuf/io-bmp.c:572
msgid "BMP image has bogus header data"
msgstr "BMP суретінің тақырыптама деректері қате"

#: gdk-pixbuf/io-bmp.c:245 gdk-pixbuf/io-bmp.c:507
msgid "Not enough memory to load bitmap image"
msgstr "Растрлық суретті жүктеу үшін жады жеткіліксіз"

#: gdk-pixbuf/io-bmp.c:339
msgid "BMP image has unsupported header size"
msgstr "BMP суретінде қолдау көрсетілмейтін тақырыптама өлшемі бар"

#: gdk-pixbuf/io-bmp.c:348
msgid "BMP image has unsupported depth"
msgstr "BMP суретінде қолдау көрсетілмейтін тереңдік бар"

#: gdk-pixbuf/io-bmp.c:367
msgid "BMP image has oversize palette"
msgstr "BMP суретінде өлшемі тым үлкен палитра бар"

#: gdk-pixbuf/io-bmp.c:399
msgid "Topdown BMP images cannot be compressed"
msgstr "Жоғарыдан төмен бағытталған BMP суреттерін сығу мүмкін емес"

#: gdk-pixbuf/io-bmp.c:424
msgid "BMP image width too large"
msgstr "BMP суретінің ені тым үлкен"

#: gdk-pixbuf/io-bmp.c:814 gdk-pixbuf/io-png.c:587 gdk-pixbuf/io-pnm.c:722
#: gdk-pixbuf/io-pnm.c:879
msgid "Premature end-of-file encountered"
msgstr "Файлдың соңы күтпеген жерден кездесті"

#: gdk-pixbuf/io-bmp.c:1340
msgid "Error while decoding colormap"
msgstr "Түстер картасын декодтау кезіндегі қате"

#: gdk-pixbuf/io-bmp.c:1403 gdk-pixbuf/io-bmp.c:1415
msgid "Image is too wide for BMP format."
msgstr "Сурет BMP пішімі үшін тым кең."

#: gdk-pixbuf/io-bmp.c:1448
msgid "Couldn’t allocate memory for saving BMP file"
msgstr "BMP файлын сақтау үшін жадыны бөлу мүмкін емес"

#: gdk-pixbuf/io-bmp.c:1489
msgid "Couldn’t write to BMP file"
msgstr "BMP файлына жазу мүмкін емес"

#: gdk-pixbuf/io-bmp.c:1542 gdk-pixbuf/io-gdip-bmp.c:83
msgctxt "image format"
msgid "BMP"
msgstr "BMP"

#: gdk-pixbuf/io-gdip-emf.c:61
msgctxt "image format"
msgid "EMF"
msgstr "EMF"

#: gdk-pixbuf/io-gdip-gif.c:81 gdk-pixbuf/io-gif.c:1039
msgctxt "image format"
msgid "GIF"
msgstr "GIF"

#: gdk-pixbuf/io-gdip-ico.c:60 gdk-pixbuf/io-ico.c:1418
msgctxt "image format"
msgid "Windows icon"
msgstr "Windows таңбашасы"

#: gdk-pixbuf/io-gdip-jpeg.c:54 gdk-pixbuf/io-jpeg.c:1409
#, c-format
msgid ""
"JPEG quality must be a value between 0 and 100; value “%s” could not be "
"parsed."
msgstr ""
"JPEG сапасы 0 мен 100 арасындағы мән болуы тиіс; “%s” мәнін өңдеу мүмкін "
"болмады."

#: gdk-pixbuf/io-gdip-jpeg.c:69 gdk-pixbuf/io-jpeg.c:1425
#, c-format
msgid ""
"JPEG quality must be a value between 0 and 100; value “%d” is not allowed."
msgstr ""
"JPEG сапасы 0 мен 100 арасындағы мән болуы тиіс; “%d” мәніне рұқсат жоқ."

#: gdk-pixbuf/io-gdip-jpeg.c:147 gdk-pixbuf/io-jpeg.c:1710
msgctxt "image format"
msgid "JPEG"
msgstr "JPEG"

#: gdk-pixbuf/io-gdip-tiff.c:83 gdk-pixbuf/io-tiff.c:1087
msgctxt "image format"
msgid "TIFF"
msgstr "TIFF"

#: gdk-pixbuf/io-gdip-utils.c:155
#, c-format
msgid "Could not allocate memory: %s"
msgstr "Жадыны бөлу мүмкін емес: %s"

#: gdk-pixbuf/io-gdip-utils.c:180 gdk-pixbuf/io-gdip-utils.c:294
#: gdk-pixbuf/io-gdip-utils.c:334
#, c-format
msgid "Could not create stream: %s"
msgstr "Ағынды жасау мүмкін емес: %s"

#: gdk-pixbuf/io-gdip-utils.c:194
#, c-format
msgid "Could not seek stream: %s"
msgstr "Ағын бойынша өту мүмкін емес: %s"

#: gdk-pixbuf/io-gdip-utils.c:206
#, c-format
msgid "Could not read from stream: %s"
msgstr "Ағыннан оқу мүмкін емес: %s"

#: gdk-pixbuf/io-gdip-utils.c:620
msgid "Couldn’t load bitmap"
msgstr "Биттік картаны жүктеу мүмкін болмады"

#: gdk-pixbuf/io-gdip-utils.c:776
msgid "Couldn’t load metafile"
msgstr "Метафайлды жүктеу мүмкін болмады"

#: gdk-pixbuf/io-gdip-utils.c:881
msgid "Unsupported image format for GDI+"
msgstr "GDI+ үшін қолдау көрсетілмейтін сурет пішімі"

#: gdk-pixbuf/io-gdip-utils.c:888
msgid "Couldn’t save"
msgstr "Сақтау мүмкін емес"

#: gdk-pixbuf/io-gdip-wmf.c:60
msgctxt "image format"
msgid "WMF"
msgstr "WMF"

#: gdk-pixbuf/io-gif.c:160
#, c-format
msgid "Failure reading GIF: %s"
msgstr "GIF оқу қатесі: %s"

#: gdk-pixbuf/io-gif.c:383 gdk-pixbuf/io-gif.c:850 gdk-pixbuf/io-gif.c:903
#: gdk-pixbuf/io-gif.c:976
msgid "Not enough memory to load GIF file"
msgstr "GIF файлын жүктеу үшін жады жеткіліксіз"

#: gdk-pixbuf/io-gif.c:509
msgid "GIF image is corrupt (incorrect LZW compression)"
msgstr "GIF суреті зақымдалған (дұрыс емес LZW сығуы)"

#: gdk-pixbuf/io-gif.c:545
msgid "File does not appear to be a GIF file"
msgstr "Файл GIF файлына ұқсамайды"

#: gdk-pixbuf/io-gif.c:560
#, c-format
msgid "Version %s of the GIF file format is not supported"
msgstr "GIF файл пішімінің %s нұсқасына қолдау көрсетілмейді"

#: gdk-pixbuf/io-gif.c:589
msgid "Resulting GIF image has zero size"
msgstr "Шыққан GIF суретінің өлшемі нөлге тең"

#: gdk-pixbuf/io-gif.c:659
msgid ""
"GIF image has no global colormap, and a frame inside it has no local "
"colormap."
msgstr ""
"GIF суретінде глобалды түстер картасы жоқ, және оның ішіндегі кадрдың "
"жергілікті түстер картасы жоқ."

#: gdk-pixbuf/io-gif.c:863 gdk-pixbuf/io-gif.c:988
msgid "GIF file was missing some data (perhaps it was truncated somehow?)"
msgstr ""
"GIF файлында кейбір деректер жетіспейді (мүмкін ол қандай да бір жолмен "
"қиылған ба?)"

#: gdk-pixbuf/io-gif.c:922
msgid "GIF image was truncated or incomplete."
msgstr "GIF суреті қиылған немесе толық емес."

#: gdk-pixbuf/io-gif.c:929
msgid "Not all frames of the GIF image were loaded."
msgstr "GIF суретінің барлық кадрлары жүктелмеді."

#: gdk-pixbuf/io-icns.c:363
#, c-format
msgid "Error reading ICNS image: %s"
msgstr "ICNS суретін оқу қатесі: %s"

#: gdk-pixbuf/io-icns.c:380 gdk-pixbuf/io-icns.c:461
msgid "Could not decode ICNS file"
msgstr "ICNS файлын декодтау мүмкін болмады"

#: gdk-pixbuf/io-icns.c:517
msgctxt "image format"
msgid "MacOS X icon"
msgstr "MacOS X таңбашасы"

#: gdk-pixbuf/io-ico.c:238 gdk-pixbuf/io-ico.c:252 gdk-pixbuf/io-ico.c:342
#: gdk-pixbuf/io-ico.c:426 gdk-pixbuf/io-ico.c:451
#, c-format
msgid "Invalid header in icon (%s)"
msgstr "Таңбаша тақырыптамасы жарамсыз (%s)"

#: gdk-pixbuf/io-ico.c:268 gdk-pixbuf/io-ico.c:355 gdk-pixbuf/io-ico.c:461
#: gdk-pixbuf/io-ico.c:504 gdk-pixbuf/io-ico.c:532
msgid "Not enough memory to load icon"
msgstr "Таңбашаны жүктеу үшін жады жеткіліксіз"

#: gdk-pixbuf/io-ico.c:386
msgid "Invalid header in icon"
msgstr "Таңбаша тақырыптамасы жарамсыз"

#: gdk-pixbuf/io-ico.c:387
msgid "Compressed icons are not supported"
msgstr "Сығылған таңбашаларға қолдау көрсетілмейді"

#: gdk-pixbuf/io-ico.c:489
msgid "Unsupported icon type"
msgstr "Қолдауы жоқ таңбаша түрі"

#: gdk-pixbuf/io-ico.c:583
msgid "Not enough memory to load ICO file"
msgstr "ICO файлын жүктеу үшін жады жеткіліксіз"

#: gdk-pixbuf/io-ico.c:629
msgid "ICO image was truncated or incomplete."
msgstr "ICO суреті қиылған немесе толық емес."

#: gdk-pixbuf/io-ico.c:1070
msgid "Image too large to be saved as ICO"
msgstr "Сурет ICO ретінде сақтау үшін тым үлкен"

